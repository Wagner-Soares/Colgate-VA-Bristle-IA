                    </Grid.ColumnDefinitions>
                    <Grid Grid.Column="0" Background="White" Height="43" VerticalAlignment="Bottom">
                        <Button x:Name="btnIPPrediction" Content="IP Prediction:" Background="DarkCyan" BorderBrush="#FF69C6C6" Margin="0,0,0,5" Height="32" VerticalAlignment="Bottom" Click="TestIpPredictionConnection"/>
                    </Grid>
                    <Grid Grid.Column="1" Background="White" Height="43" VerticalAlignment="Bottom">
                        <TextBox x:Name="IpPrediction" FontSize="17" IsReadOnly="false" Text="" CaretBrush="#FF686868" Foreground="Black" Margin="10,0,10,9" VerticalAlignment="Bottom" TextChanged="TestIpPredictionConnection"/>
                    </Grid>
                    <Grid Grid.Column="2" Background="White" Height="43" VerticalAlignment="Bottom">
                        <Button x:Name="btnPortPredition" Content="Port Prediction:" Background="DarkCyan" BorderBrush="#FF69C6C6" Margin="0,0,0,5" VerticalAlignment="Bottom" Click="BtnPortPrediction_Click"/>
                    </Grid>
                    <Grid Grid.Column="3" Background="White" Height="43" VerticalAlignment="Bottom">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="13*"/>
