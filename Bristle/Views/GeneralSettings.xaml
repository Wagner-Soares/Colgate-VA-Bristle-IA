                    </Grid.ColumnDefinitions>
                    <Grid Grid.Column="0" Background="White" Margin="0,0,0,0">
                        <Button Content="Group Names:" Background="DarkCyan" BorderBrush="#FF69C6C6"  VerticalAlignment="Center"/>
                    </Grid>
                    <Grid Grid.Column="1" Background="White" Margin="1,0,1,0">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="1*"/>
                            <RowDefinition Height="1*"/>
                        </Grid.RowDefinitions>
                        <Grid Grid.Row="0">
                            <Button Content="Admins AD Group" Background="DarkCyan" BorderBrush="#FF69C6C6" VerticalAlignment="Center"/>
                        </Grid>
                        <Grid Grid.Row="1">
                            <TextBox x:Name="txbAdminsADGroup" IsReadOnly="false" Text="" CaretBrush="#FF686868" Foreground="Black" Margin="0,0,0,1" VerticalAlignment="Bottom" TextAlignment="Center"  TextDecorations="{x:Null}"  />
                        </Grid>
                    </Grid>
                    <Grid Grid.Column="2" Background="White" Margin="1,0,1,0">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="1*"/>
                            <RowDefinition Height="1*"/>
                        </Grid.RowDefinitions>
                        <Grid Grid.Row="0">
                            <Button Content="Quality AD Group" Background="DarkCyan" BorderBrush="#FF69C6C6" VerticalAlignment="Center"/>
                        </Grid>
                        <Grid Grid.Row="1">
                            <TextBox x:Name="txbQualityADGroup" IsReadOnly="false" Text="" CaretBrush="#FF686868" Foreground="Black" Margin="0,0,0,1" VerticalAlignment="Bottom" TextAlignment="Center" TextDecorations="{x:Null}"  />
                        </Grid>
                    </Grid>
                    <Grid Grid.Column="3" Background="White" Margin="1,0,1,0">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="1*"/>
                            <RowDefinition Height="1*"/>
                        </Grid.RowDefinitions>
                        <Grid Grid.Row="0">
                            <Button Content="Operators AD Group" Background="DarkCyan" BorderBrush="#FF69C6C6" VerticalAlignment="Center"/>
                        </Grid>
                        <Grid Grid.Row="1">
                            <TextBox x:Name="txbOperatorsADGroup" IsReadOnly="false" Text="" CaretBrush="#FF686868" Foreground="Black" Margin="0,0,0,1" VerticalAlignment="Bottom" TextAlignment="Center" TextDecorations="{x:Null}"  />
                        </Grid>
                    </Grid>
                </Grid>

                <Grid Grid.Row="13" Background="White">
                    <TextBlock Text="Camera Configuration" HorizontalAlignment="Center" Foreground="#FF050505" FontSize="21" VerticalAlignment="Center" FontWeight="Bold"/>
                </Grid>
                <Grid Grid.Row="14" Background="White" HorizontalAlignment="Left" Width="632">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="2*"/>
                    </Grid.ColumnDefinitions>
                    <Grid Grid.Column="0">
                        <ComboBox x:Name="cameraSelect" ToolTip="Select Camera" VerticalAlignment="Top" Margin="17,0,0,0" SelectionChanged="ComboBox0_SelectionChanged" Height="30" BorderBrush="#FF030303" Foreground="#FF030303" Background="DarkCyan" >
                        </ComboBox>
                    </Grid>
                </Grid>
                <Grid Grid.Row="15" Background="White" HorizontalAlignment="Left" Width="632" Margin="0,0,0,5">
