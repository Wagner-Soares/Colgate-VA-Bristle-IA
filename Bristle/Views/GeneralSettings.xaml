                        </Grid.RowDefinitions>
                        <TextBox x:Name="PortPrediction" FontSize="17" IsReadOnly="false" Text="" CaretBrush="#FF686868" Foreground="Black" Margin="10,0,10,10" VerticalAlignment="Bottom" Grid.RowSpan="2"  />
                    </Grid>
                </Grid>
