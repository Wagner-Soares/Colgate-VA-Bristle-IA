                        <ComboBox x:Name="selectResolution" ToolTip="Select Resolution" VerticalAlignment="Top" Margin="17,0,0,0" SelectionChanged="ComboBox1_SelectionChanged" Height="30" BorderBrush="#FF030303" Foreground="#FF030303" Background="DarkCyan" >
                        </ComboBox>
                    </Grid>
                </Grid>
