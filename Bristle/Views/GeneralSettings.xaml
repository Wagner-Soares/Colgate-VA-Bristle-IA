                        <Grid Grid.Row="1">
                            <TextBox x:Name="txbtuftM2BristleCountTestName" IsReadOnly="false" Text="" CaretBrush="#FF686868" Foreground="Black" Margin="0,0,0,1" VerticalAlignment="Bottom" TextAlignment="Center" TextDecorations="{x:Null}"  />
                        </Grid>
                    </Grid>
                    <Grid Grid.Column="5" Background="White" Margin="1,0,1,0">
                        <Grid.RowDefinitions>
