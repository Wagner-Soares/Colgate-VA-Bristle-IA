                            <RowDefinition Height="1*"/>
                        </Grid.RowDefinitions>
                        <Grid Grid.Row="0">
                            <Button Content="Tuft M3 Bristle Count" Background="DarkCyan" BorderBrush="#FF69C6C6" VerticalAlignment="Center"/>
                        </Grid>
                        <Grid Grid.Row="1">
                            <TextBox x:Name="txbtuftM3BristleCountTestName" IsReadOnly="false" Text="" CaretBrush="#FF686868" Foreground="Black" Margin="0,0,0,1" VerticalAlignment="Bottom" TextAlignment="Center" TextDecorations="{x:Null}"  />
                        </Grid>
                    </Grid>
                    <Grid Grid.Column="6" Background="White" Margin="1,0,1,0">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="1*"/>
                            <RowDefinition Height="1*"/>
                        </Grid.RowDefinitions>
                        <Grid Grid.Row="0">
                            <Button Content="Tuft N Bristle Count" Background="DarkCyan" BorderBrush="#FF69C6C6" VerticalAlignment="Center"/>
                        </Grid>
                        <Grid Grid.Row="1">
                            <TextBox x:Name="txbtuftNBristleCountTestName" IsReadOnly="false" Text="" CaretBrush="#FF686868" Foreground="Black" Margin="0,0,0,1" VerticalAlignment="Bottom" TextAlignment="Center" TextDecorations="{x:Null}"  />
                        </Grid>
                    </Grid>
                </Grid>
                <Grid Grid.Row="12">
