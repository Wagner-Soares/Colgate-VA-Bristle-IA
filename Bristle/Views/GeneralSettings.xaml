                    <!--<Grid Grid.Column="0" Background="White" Height="43" VerticalAlignment="Bottom">
                        -->
                    <!--<Button Content="IP Interface:" Background="DarkCyan" BorderBrush="#FF69C6C6" Margin="0,0,0,5" Height="32" VerticalAlignment="Bottom"/>-->
                    <!--
                    </Grid>
                    <Grid Grid.Column="1" Background="White" Height="43" VerticalAlignment="Bottom">
                        -->
                    <!--<TextBox x:Name="IpInterface" FontSize="17" IsReadOnly="false" Text="" CaretBrush="#FF686868" Foreground="Black" Margin="10,0,10,9" VerticalAlignment="Bottom" />-->
                    <!--
                    </Grid>-->
