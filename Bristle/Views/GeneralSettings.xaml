                    <TextBlock Text="Network Configuration" HorizontalAlignment="Center" Foreground="#FF050505" FontSize="21" VerticalAlignment="Center" FontWeight="Bold"/>
                </Grid>
                <Grid Grid.Row="2">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="5*"/>
                        <ColumnDefinition Width="8*"/>
